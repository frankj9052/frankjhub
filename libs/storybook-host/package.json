{
  "name": "@frankjhub/storybook-host",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "nx": {
    "targets": {
      "storybook": {
        "options": {
          "port": 9019
        }
      },
      "storybook-static": {
        "executor": "@nx/web:file-server",
        "options": {
          "staticFilePath": "libs/storybook-host/storybook-static",
          "port": 9009,
          "fallback": "index.html",
          "spa": true,
          "watch": false
        }
      }
    }
  }
}
