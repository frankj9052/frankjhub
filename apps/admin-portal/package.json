{
  "name": "@frankjhub/admin-portal",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "lodash": "4.17.21",
    "lodash.debounce": "4.0.8",
    "next": "~15.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.33.5",
    "typescript": "5.7.3"
  },
  "nx": {
    "projectType": "application",
    "sourceRoot": "apps/admin-portal",
    "targets": {
      "dev": {
        "defaultConfiguration": "development"
      },
      "start": {
        "defaultConfiguration": "production"
      },
      "start:only": {
        "executor": "nx:run-commands",
        "defaultConfiguration": "production",
        "options": {
          "command": "next start apps/admin-portal"
        }
      },
      "build:only": {
        "executor": "@nx/next:build",
        "dependsOn": [],
        "defaultConfiguration": "production",
        "options": {
          "root": "apps/admin-portal",
          "outputPath": "apps/admin-portal",
          "buildLibsFromSource": true
        }
      },
      "collect-source": {
        "executor": "nx:run-commands",
        "options": {
          "command": "tsx scripts/collectSourceFiles.ts",
          "cwd": "apps/admin-portal"
        }
      }
    }
  },
  "overrides": {
    "storybook": "$storybook"
  },
  "scripts": {
    "start": "next start"
  }
}