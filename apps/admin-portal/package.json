{
  "name": "@frankjhub/admin-portal",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "next": "~15.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "nx": {
    "projectType": "application",
    "sourceRoot": "apps/admin-portal",
    "targets": {
      "dev": {   
        "defaultConfiguration": "development"
      },
      "start": {
        "defaultConfiguration": "production"
      },
      "collect-source": {
        "executor": "nx:run-commands",
        "options": {
          "command": "tsx scripts/collectSourceFiles.ts",
          "cwd": "apps/admin-portal"
        }
      }
    }
  }
}
