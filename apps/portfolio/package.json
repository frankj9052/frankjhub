{
  "name": "@frankjhub/portfolio",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "next": "~15.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "nx": {
    "projectType": "application",
    "targets": {
      "dev": {
        "defaultConfiguration": "development"
      },
      "start": {
        "defaultConfiguration": "production"
      },
      "start:only": {
        "executor": "nx:run-commands",
        "defaultConfiguration": "production",
        "options": {
          "command": "next start apps/portfolio"
        }
      }
    }
  }
}
